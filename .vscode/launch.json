{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch BACK",
      "runtimeExecutable": "yarn",
      "cwd": "${workspaceFolder}/server",
      "runtimeArgs": [
        "dev:debug"
      ],
    }
  ]
}
