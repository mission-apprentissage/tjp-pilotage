  "repository": "https://github.com/mission-apprentissage/tjp-pilotage.git",
  "version": "0.1.0",
  "author": "MNA",
  "license": "MIT",
  "private": true,
  "scripts": {
    "docker-dev": "yarn install && yarn dev",
    "dev": "NEXT_PUBLIC_VERSION=$npm_package_version next dev",
