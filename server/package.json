{
  "name": "server",
  "version": "0.0.1",
  "description": "[MNA] pilotage server",
  "repository": "https://github.com/mission-apprentissage/tjp-pilotage.git",
  "private": true,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup src/index.ts --watch . --onSuccess 'yarn run start'",
    "build": "tsup src/index.ts",
    "create-migration": "knex migrate:make",
    "create-migration-docker": "docker exec -it pilotage_server yarn create-migration",
    "migrate": "knex migrate:latest",
    "migrate-docker": "docker exec -it pilotage_server yarn migrate",
    "zapatos": "zapatos",
    "zapatos-docker": "docker exec -it pilotage_server yarn zapatos"
  },
  "type": "module",
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "axios": "0.24.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "bunyan-slack": "0.0.10",
    "dotenv": "16.0.0",
    "env-var": "7.1.1",
    "fastify": "^4.12.0",
    "jsonwebtoken": "8.5.1",
    "knex": "^2.4.2",
    "luxon": "2.3.0",
    "pg": "^8.9.0",
    "rate-limiter-flexible": "2.3.6",
    "shared": "*",
    "uuid": "9.0.0",
    "zapatos": "^6.1.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/pg": "^8.6.6",
    "chalk": "4.1.2",
    "nodemon": "^2.0.20",
    "tsup": "^6.5.0",
    "typescript": "4.9.4"
  }
}
