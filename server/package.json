{
  "name": "tjp-pilotage-server",
  "version": "0.0.1",
  "description": "[MNA] pilotage server",
  "repository": "https://github.com/mission-apprentissage/tjp-pilotage.git",
  "private": false,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --ignore tests/ src/index.js",
    "debug": "nodemon --inspect=0.0.0.0 --signal SIGINT --ignore tests/ src/index.js",
    "test": "PILOTAGE_LOG_LEVEL=fatal mocha --exit tests/**/*-test.js",
    "test:coverage": "nyc --temp-dir coverage/.nyc_output --report-dir coverage --reporter=lcov --reporter=html yarn test",
    "cli": "PILOTAGE_LOG_DESTINATIONS=stderr node src/cli.js",
    "lint": "eslint src/ tests/"
  },
  "dependencies": {
    "axios": "0.24.0",
    "body-parser": "1.20.0",
    "boom": "7.3.0",
    "bunyan": "1.8.15",
    "bunyan-slack": "0.0.10",
    "bunyan-mongodb-stream": "0.0.3",
    "bunyan-prettystream": "0.1.3",
    "commander": "9.2.0",
    "compose-middleware": "5.0.1",
    "dotenv": "16.0.0",
    "env-var": "7.1.1",
    "cookie-parser": "1.4.6",
    "express": "4.18.1",
    "joi": "17.6.0",
    "lodash-es": "4.17.21",
    "mongodb": "4.5.0",
    "oleoduc": "0.7.0",
    "pretty-ms": "7.0.1",
    "ejs": "3.1.7",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "luxon": "2.3.0",
    "mjml": "4.11.0",
    "nodemailer": "6.7.2",
    "nodemailer-html-to-text": "3.2.0",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "rate-limiter-flexible": "2.3.6",
    "sha512crypt-node": "0.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "axiosist": "0.10.0",
    "chalk": "4.1.2",
    "eslint": "8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-mocha": "10.0.4",
    "lint-staged": ">=10",
    "mocha": "10.0.0",
    "mongodb-memory-server": "8.5.2",
    "nock": "13.2.4",
    "nodemon": "2.0.16",
    "nyc": "15.1.0",
    "prettier": "2.6.2"
  }
