{
  "name": "server",
  "version": "0.0.1",
  "description": "[MNA] pilotage server",
  "repository": "https://github.com/mission-apprentissage/tjp-pilotage.git",
  "private": true,
  "author": "MNA",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "node --enable-source-maps dist/index.js",
    "dev": "tsup src/index.ts --watch . --watch ../shared --onSuccess 'yarn run copyPublic && yarn run start'",
    "copyPublic": "cp -a public/. dist",
    "build": "tsup src/index.ts",
    "typecheck": "tsc",
    "cli": "ts-node -T src/cli.ts ",
    "kysely": "npx kysely-codegen --schema public --out-file=./src/db/schema.ts --dialect postgres",
    "test": "jest"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/swagger": "^8.3.1",
    "@fastify/swagger-ui": "^1.6.0",
    "@fastify/type-provider-typebox": "^3.5.0",
    "@hapi/boom": "^10.0.1",
    "@json2csv/node": "^6.1.3",
    "@json2csv/plainjs": "^6.1.3",
    "axios": "1.3.5",
    "commander": "^10.0.0",
    "cookie": "^0.5.0",
    "csv-parse": "^5.3.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "env-var": "7.1.1",
    "fastify": "^4.23.2",
    "fastify-qs": "^4.0.1",
    "jsonwebtoken": "8.5.1",
    "kysely": "0.26.0",
    "lodash": "^4.17.21",
    "luxon": "2.3.0",
    "mjml": "^4.14.1",
    "nodemailer": "^6.9.4",
    "nodemailer-html-to-text": "^3.2.0",
    "pg": "^8.9.0",
    "qs": "^6.11.1",
    "rate-limiter-flexible": "2.3.6",
    "shared": "*",
    "stream-transform": "^3.2.2",
    "ts-node": "^10.9.1",
    "tsup": "^6.5.0",
    "uglify-js": "^3.17.4",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "winston-slack-webhook-transport": "^2.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "@types/ejs": "^3.1.2",
    "@types/jest": "^29.5.1",
    "@types/json2csv": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.191",
