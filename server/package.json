    "@types/nodemailer": "^6.4.9",
    "@types/nodemailer-html-to-text": "^3.1.0",
    "@types/pg": "^8.6.6",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^9.0.4",
    "esbuild-jest": "^0.5.0",
    "jest": "^29.5.0",
    "kysely-codegen": "^0.10.0",
    "ts-jest": "^29.1.0",
    "typescript": "4.9.4"
  }
