{
  "extends": ["plugin:n/recommended-module", "plugin:import/recommended", "plugin:import/typescript"],
  "env": {
    "es2022": true,
    "node": true
  },
  "plugins": ["zod"],
  "rules": {
    "zod/prefer-enum": "error",
    "n/no-missing-import": 0,
    "n/no-extraneous-import": ["error"]
  }
}
